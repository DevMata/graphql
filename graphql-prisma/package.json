{
  "name": "graphql-course",
  "version": "1.0.0",
  "description": "A graphql companion repo for a graphql-course",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.js --ext js,graphql --exec babel-node",
    "start:debug": "nodemon --inspect-brk src/index.js --ext js,graphql --exec babel-node",
    "codegen": "graphql codegen",
    "prisma": "node node_modules/prisma1/dist/index.js",
    "prisma:deploy": "npm run prisma -- deploy -p"
  },
  "keywords": [
    "graphql",
    "prisma",
    "graphql-yoga"
  ],
  "author": "Antonio Mata <antonio.mata.sv@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "graphql": "14.6.0",
    "graphql-binding": "^2.5.2",
    "graphql-cli": "^4.1.0",
    "graphql-yoga": "^1.18.3",
    "prisma-binding": "^2.3.16",
    "prisma1": "^1.34.12",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@graphql-cli/codegen": "^1.17.16",
    "@graphql-codegen/schema-ast": "^1.18.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1"
  }
}
